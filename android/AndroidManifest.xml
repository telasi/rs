<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ge.telasy.reading.system"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk android:minSdkVersion="15" />
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.SEND_SMS"/>

    <application
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name" >
        <activity
            android:name="ge.telasy.reading.system.login"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity 
             android:name="ge.telasy.reading.system.enterCountersShow"
             android:theme="@android:style/Theme.Black.NoTitleBar.Fullscreen" 
             android:stateNotNeeded="false">
        </activity>
        <activity
             android:name="ge.telasy.reading.system.TARSAktivity"
             android:screenOrientation="landscape"
             android:stateNotNeeded="false"
             android:label="@string/header_title"
             >
        </activity>
        <activity android:name="ge.telasy.reading.system.AdminlayoutActivity"></activity>
        <activity android:name="ge.telasy.reading.system.FromQuery"></activity>
        <activity android:name="ge.telasy.reading.system.routedownload"></activity>
        <activity 
                android:name="ge.telasy.reading.system.routemanager"
                android:label="@string/route_manage">
        </activity>
        <activity android:name="ge.telasy.reading.system.outOfReester"></activity>
        <activity 
                 android:name="ge.telasy.reading.system.UserManager"
                 android:label="@string/app_user_name"
         >
        </activity>
         
    </application>

</manifest>

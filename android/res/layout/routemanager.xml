<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#006633"
    android:orientation="vertical" >
    <TextView
             android:layout_width="600dp"
             android:layout_height="wrap_content"
             android:text="@string/route_upH"
             android:textSize="25sp"
             android:textColor="#ffffff"
             android:orientation="vertical"
             android:gravity="center"
             android:paddingLeft="25dp"
             android:paddingBottom="15dp"/>
    
    <LinearLayout 
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:background="#ffffff"
        android:orientation="horizontal">

        <TextView
            android:layout_width="150dp"
            android:layout_height="wrap_content"
            android:text="@string/amc"
            android:gravity="center"
            android:paddingTop="15dp"
            android:textColor="#000000"
            android:textSize="20sp"/>
        <LinearLayout 
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="#ffffff"
            android:orientation="vertical" >
            <TextView
                android:layout_width="300dp"
                android:layout_height="wrap_content"
                android:text="@string/Route_lab"
                android:textColor="#000000"
                android:gravity="center"
                android:textSize="20sp"/>
            <LinearLayout 
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="#ffffff"
                android:orientation="horizontal" >
                <TextView
                   android:layout_width="200dp"
                   android:layout_height="wrap_content"
                   android:text="@string/dasaxeleba"
                   android:textColor="#000000"
                   android:textSize="20sp"/>
                <TextView
                   android:layout_width="100dp"
                   android:layout_height="wrap_content"
                   android:text="@string/nomeri"
                   android:textColor="#000000"
                   android:textSize="20sp"/>
            </LinearLayout>
        </LinearLayout>
        <TextView
             android:layout_width="100dp"
             android:layout_height="wrap_content"
             android:text="@string/wakiTxuli"
             android:gravity="center_horizontal"
             android:textColor="#000000"
             android:textSize="20sp"/>
        <TextView
             android:layout_width="200dp"
             android:layout_height="wrap_content"
             android:text="@string/saeTvo"
             android:textColor="#000000"
             android:textSize="20sp"/>
        <TextView
             android:layout_width="150dp"
             android:layout_height="wrap_content"
             android:text="@string/lbtarigi"
             android:textColor="#000000"
             android:textSize="20sp"/>
        <TextView
             android:layout_width="270dp"
             android:layout_height="wrap_content"
             android:text="@string/wamkiTxveli"
             android:textColor="#000000"
             android:textSize="20sp"/>    
        <TextView
             android:layout_width="100dp"
             android:layout_height="wrap_content"
              android:gravity="center_horizontal"
             android:text="@string/ststus"
             android:textColor="#000000"
             android:textSize="20sp"/>         
    </LinearLayout>
    <!-- ???????????????????????????????????????? -->
    <ListView
       android:id="@+id/android:list"
       android:layout_width="wrap_content"
       android:layout_height="0dip"
       android:background="#006600"
       android:listSelector="#e4137f"
       android:layout_weight="1" >

    </ListView>
    <TextView
       android:id="@android:id/empty" 
       android:textSize="20sp"
       android:gravity="center"
       android:layout_width="340dip"
       android:layout_height="wrap_content"
       android:text="@string/listisempty"
       android:textColor="#ffffff"/>   
        
    <LinearLayout 
        android:layout_width="fill_parent"
        android:layout_height="75dp"
        android:background="#ffffff"
        android:orientation="horizontal"
        android:paddingBottom="10sp" >
         
        <LinearLayout 
             android:layout_width="fill_parent"
             android:layout_height="wrap_content"
             android:orientation="horizontal" 
             android:background="#ffffff"
             android:paddingBottom="15dp">

             <Button
                 android:id="@+id/b_goToReesterdownload"
                 android:layout_height="wrap_content"
                 android:layout_width="640sp"
                 android:onClick="goToReesterdownload"
                 android:paddingLeft="100sp"
                 android:paddingRight="100dp"
                 android:text="@string/goToReesterdownload"
                 android:textColor="#000000"
                 android:textSize="22sp" />

             <Button
                 android:id="@+id/b_goToMainReester"
                 android:layout_width="640dp"
                 android:layout_height="wrap_content"
                 android:textSize="22sp"
                 android:textColor="#000000"
                 android:paddingLeft="200dp"
                 android:paddingRight="200dp"
                 android:onClick="goToMainReester"
                 android:text="@string/goToMainReester" />
             
        </LinearLayout> 
    </LinearLayout>
</LinearLayout>
<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
     xmlns:android="http://schemas.android.com/apk/res/android"
     android:layout_width="wrap_content"
     android:layout_height="wrap_content"
     android:dividerPadding="10dp"
     android:divider="#ffffff"
     android:orientation="horizontal">
     
     <TextView
         android:id="@+id/amcId"
         android:layout_width="220dp"
         android:layout_height="50dp"
         android:text="@string/amc"
         android:textSize="20sp"
         android:paddingLeft="5dp"
         android:maxLines="2"
         android:minLines="1"
         />

      <TextView
         android:id="@+id/marshNameId"
         android:layout_width="250dp"
         android:layout_height="50dp"
         android:text="@string/mars_dasaxeleba"
         android:textSize="20sp"
         android:maxLines="2"
         android:minLines="1"
         android:paddingLeft="5dp"
         />
      <TextView
         android:id="@+id/marshNumId"
         android:layout_width="100dp"
         android:layout_height="50dp"
         android:text="@string/marshnum"
         android:textSize="20sp"
         android:paddingLeft="5dp"/>
      <TextView
         android:id="@+id/wamkitxId"
         android:layout_width="170dp"
         android:layout_height="50dp"
         android:text="@string/wamkiTxveli"
         android:paddingLeft="5dp"
         android:textSize="20sp"/>
      
      <TextView
         android:id="@+id/tarigiId"
         android:layout_width="120dp"
         android:layout_height="50dp"
         android:text="@string/lbtarigi"
         android:paddingLeft="5dp"
         android:textSize="20sp"/>
      
      <TextView
         android:id="@+id/statusiId"
         android:layout_width="200dp"
         android:layout_height="50dp"
         android:text="@string/statusi"
         android:paddingLeft="5dp"
         android:textSize="20sp"/>
     
     <TextView
         android:id="@+id/wasakitxiId"
         android:layout_width="200dp"
         android:layout_height="50dp"
         android:paddingLeft="5dp"
         android:text="@string/wasakitxi"
         android:textSize="20sp"/>

     
</LinearLayout>








